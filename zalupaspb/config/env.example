# Пример файла .env
# Скопируйте в .env и отредактируйте

# Настройки Django
DJANGO_SECRET_KEY=your-super-secret-key-here
DJANGO_DEBUG=False
DJANGO_ALLOWED_HOSTS=dinozavrikgugl.ru,www.dinozavrikgugl.ru

# Настройки базы данных
DB_NAME=zalupaspb
DB_USER=postgres
DB_PASSWORD=your-db-password
DB_HOST=localhost
DB_PORT=5432

# Настройки Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Настройки CORS
CORS_ALLOWED_ORIGINS=http://dinozavrikgugl.ru,http://localhost:8000

# Настройки API для бота
API_URL=http://dinozavrikgugl.ru/api
API_TOKEN=ваш_токен_доступа
API_REFRESH_TOKEN=ваш_токен_обновления

# Настройки Discord
DISCORD_BOT_TOKEN=your-discord-bot-token
DISCORD_GUILD_ID=123456789012345678
DISCORD_LOG_CHANNEL_ID=123456789012345678
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/...

# Настройки ролей Discord
DISCORD_ADMIN_ROLE_ID=123456789012345678
DISCORD_MODERATOR_ROLE_ID=123456789012345678
DISCORD_SUPPORT_ROLE_ID=123456789012345678
DISCORD_USER_ROLE_ID=123456789012345678 